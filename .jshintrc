{
    "bitwise": true,
    "camelcase": true,
    "curly": true,
    "eqeqeq": true,
    "freeze": true,
    "immed": true,
    "indent": 4,
    "latedef": "nofunc",
    "newcap": true,
    "noarg": true,
    "nonew": true,
    "quotmark": "single",
    "undef": true,
    "unused": "vars",
    "strict": true,
    "trailing": true,
    "maxparams": 4,
    "node": true,
    "overrides": {
      "test/*": {"mocha": true},
      "benchmark/*": {
        "globals": {
          "suite": false,
          "bench": false,
          "set": false
        }
      }
    }
}
